{
  "extends": "airbnb-base",
  "globals": {
    "artifacts": true,
    "contract": true,
    "web3": true,
    "assert": true,
    "it": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
  },
  "rules": {
    "strict": 0,
    "indent": ["error", 2],
    "func-names": "off",
    "max-len": "off",
    "no-underscore-dangle": "off",
    "quotes": ["error", "double"],
    "comma-dangle": 0,
    "no-console": 0,
    "operator-linebreak": 0,
  },
}
